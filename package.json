{
  "name": "realestate-demo",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "typecheck": "cross-env NODE_OPTIONS=--no-deprecation tsc",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "db:seed": "cross-env NODE_OPTIONS=--no-deprecation payload run src/db/seed.ts"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "3.33.0",
    "@payloadcms/next": "3.33.0",
    "@payloadcms/payload-cloud": "3.33.0",
    "@payloadcms/richtext-lexical": "3.33.0",
    "@payloadcms/ui": "3.33.0",
    "@radix-ui/react-avatar": "^1.1.9",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@tailwindcss/postcss": "^4.1.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "csv-parser": "^3.2.0",
    "embla-carousel-react": "^8.6.0",
    "graphql": "^16.8.1",
    "lucide-react": "^0.503.0",
    "mime-types": "^3.0.1",
    "next": "15.3.0",
    "next-themes": "^0.4.6",
    "payload": "3.33.0",
    "postcss": "^8.5.3",
    "react": "19.1.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.56.4",
    "sharp": "0.32.6",
    "slugify": "^1.6.6",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@faker-js/faker": "^9.7.0",
    "@types/json-schema": "^7.0.15",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.5.4",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "drizzle-orm": "^0.43.1",
    "drizzle-seed": "^0.3.1",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.3.0",
    "json-schema": "^0.4.0",
    "prettier": "^3.4.2",
    "tw-animate-css": "^1.2.8",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
